<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    
    <item android:state_pressed="true">
         <shape android:shape="rectangle">
            <corners android:radius="@dimen/skill_tag_radius"/>
            <stroke android:width="@dimen/stroke_width" android:color="@color/tag_jingtong"/>
            <gradient android:startColor="@color/row_grey"
                android:endColor="@color/row_grey"/>
        </shape>
    </item>
    
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/skill_tag_radius"/>
            <stroke android:width="@dimen/stroke_width" android:color="@color/tag_jingtong"/>
            <gradient android:startColor="@android:color/white"
                android:endColor="@android:color/white"/>
        </shape>
    </item>
	
</selector>
